<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> {{ site_name }} {{ cluster_name }} Openshift Health-Check Report</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            margin: 0;
            padding: 0;
            display: flex;
            flex-direction: column;
            min-height: 100vh;
        }
        .ocp .header, .ocp .footer {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 10px 20px;
            background-color: grey;
            color: white;
        }
       .ocp .header h1, .ocp .footer {
            margin: 0;
            font-size: 24px;
            text-align: center;
            flex-grow: 1;
        }
        .ocp .header h2 {
            margin: 0;
            font-size: 10px;
            text-align: center;
            flex-grow: 1;
        }
        .ocp .header .date {
            display: flex;
            flex-direction: column;
            align-items: center;
        }
        .ocp .header .logo {
            width: 200px;
            height: 52px;
        }
        .ocp .footer {
            font-size: 8px;
            padding: 5px 20px;
        }
        .ocp .date {
            font-size: 14px;
            text-align: right;
        }
        .ocp .tabs {
            display: flex;
            justify-content: center;
            background-color: #333;
            flex-wrap: wrap;
            border-bottom: 2px solid #ddd;
        }
        .ocp .tabs button {
            background-color: #444;
            color: white;
            padding: 5px 10px;
           <!-- padding: 14px 20px; -->
            border: none;
            cursor: pointer;
            transition: background-color 0.3s;
            border-radius: 12px;
            margin: 5px;
            font-weight: bold;
        }
        .ocp .tabs button:hover {
            background-color: #ddd;
            color: black;
        }
        .ocp .tab-content {
            display: none;
            padding: 5px;   /* menu uzaklik */
            flex-grow: 1;
        }
        .ocp .tab-content.active {
            display: block;
        }
        .ocp .grid-container {
            display: grid;
            grid-template-columns: 1fr 1fr;
            grid-template-rows: 1fr 1fr;
            gap: 5px;
            padding: 3px;
        }
        .ocp .grid-item {
           <!-- border: 1px solid #ddd; -->
            padding: 3px;
        }
        .ocp h2 {
            text-align: center;
        }
        .ocp  table {
            width: 70%;
            margin: auto;
            border-collapse: collapse;
        }
        .ocp th, td {
            border: 1px solid #ddd;
            padding: 8px;
            text-align: center;
            white-space: nowrap;
        }
        .ocp th {
            background-color: #333;
            color: white;
        }
        @media (max-width: 600px) {
           .ocp .tabs button {
                width: 100%;
                margin: 2px 0;
            }
        }
        .ocp .footer {
            position: fixed;
            bottom: 0;
            width: 100%;
        }
        .ocp .green-box {
        <!-- background-color: lightgreen; -->
        color: black;
        border: none;
        border-radius: 15px;
        padding: 5px 10px;
         <!-- font-weight: bold; -->
        }
        .ocp .red-box {
        background-color: lightcoral;
        color: white;
        border: none;
        border-radius: 15px;
        padding: 5px 10px;
        <!-- font-weight: bold; -->
        }
        .ocp .node-warn-section, .co-warn-section {
            margin: 1; 
            padding: 1; 
        }
    </style>
    <script>
        function openTab(evt, tabName) {
            var i, tabcontent, tablinks;
            tabcontent = document.getElementsByClassName("tab-content");
            for (i = 0; i < tabcontent.length; i++) {
                tabcontent[i].style.display = "none";
            }
            tablinks = document.getElementsByClassName("tablinks");
            for (i = 0; i < tablinks.length; i++) {
                tablinks[i].className = tablinks[i].className.replace(" active", "");
            }
            document.getElementById(tabName).style.display = "block";
            evt.currentTarget.className += " active";
        }

function filterTable(tableId) {
    const table = document.getElementById(tableId);
    const rows = table.getElementsByTagName("tr");
    const inputs = table.querySelectorAll("input[type='text']");
    const filters = Array.from(inputs).map(input => input.value.toLowerCase());

    for (let i = 1; i < rows.length; i++) { 
        const cells = rows[i].getElementsByTagName("td");
        let rowVisible = true;

        for (let j = 0; j < filters.length; j++) {
             if (cells[j] && !cells[j].innerText.toLowerCase().includes(filters[j])) {
           
                rowVisible = false;
                break; 
            }
        }

        rows[i].style.display = rowVisible ? "" : "none";
    }
}

function sortTable(tableId, columnIndex, order) {
    const table = document.getElementById(tableId);
    const rows = Array.from(table.rows).slice(1);

    rows.sort((a, b) => {
        const cellA = a.cells[columnIndex].innerText;
        const cellB = b.cells[columnIndex].innerText;

        
        if (order === 'asc') {
            return cellA.localeCompare(cellB);
        } else {
            return cellB.localeCompare(cellA);
        }
    });

    
    rows.forEach(row => table.appendChild(row));
}
    </script>
</head>
<body>

<div class="ocp">
<div class="header">
    <div><img src="data:image/png;base64," alt="Logo 1" width="200" height="52"></div>
    <div class="date">
        <h1> {{ site_name }} {{ cluster_name }} Health-Check Report</h1>
        <h2>{{ dash_date }} UTC</h2>
    </div>
    <div><img src="data:image/png;base64," alt="Logo 1" width="200" height="52"></div>
</div>


<div class="tabs">
    <button class="tablinks" onclick="openTab(event, 'nodestatus')">Nodes</button>
    <div class="divider"></div>
    <button class="tablinks" onclick="openTab(event, 'nodehealth')">Node Health</button>
    <div class="divider"></div>
    <button class="tablinks" onclick="openTab(event, 'operator')">Operator</button>
    <div class="divider"></div>
    <button class="tablinks" onclick="openTab(event, 'alerts')">Alerts</button>
    <div class="divider"></div>
    <button class="tablinks" onclick="openTab(event, 'podhealth')">POD's Health</button>
</div>

<div>
  <div class="node-warn-section">
      {% if not node_critical_flag %}
      <h2 style="background-color: #d3ebdb; color: #306c3e;">The OpenShift cluster nodes are currently in a healthy state. There are no issues with nodes</h2>
      {% else %}
      <h2 style="background-color: red; color: white;">Critical: The OpenShift cluster nodes are currently in an unhealthy state! There are issues with nodes.</h2>
      {% endif %}
  </div>
  <div class="co-warn-section">
      {% if not co_warning_flag %}
      <!-- clear bar -->
      {% else %}
      <h2 style="background-color: orange; color: white;"> Warning: There are issues with cluster operators. Please check the cluster operators!</h2>
      {% endif %}
  </div>
</div>

<div id="nodestatus" class="tab-content active">
    <div class="grid-container">
        <div class="grid-item">
            <h2>Cluster Resources</h2>
            {% include './templates/get-resource.j2' %}
        </div>
        <div class="grid-item">
            <h2>Node Status</h2>
            {% include './templates/get-nodes.j2' %}
        </div>
    </div>
</div>

<div id="operator" class="tab-content">
    {% include './templates/get-co.j2' %}
</div>

<div id="nodehealth" class="tab-content">
      <h2>ETCD Pods Status</h2>
      {% include './templates/get-etcd-stats.j2' %} 
      <h2>Node Services</h2>
      {% include './templates/get-nodeconditions.j2' %}
</div>

<div id="alerts" class="tab-content">
    <h2>Alert Manager Notifications</h2>
    {% include './templates/get-alert.j2' %}
</div>

<div id="podhealth" class="tab-content">
    <h2>POD's Health</h2>
    {% include './templates/get-pod-health.j2' %}
</div>

</div>
</body>
</html>